{
  "expo": {
    "name": "u-go",
    "slug": "u-go",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "node_modules/@expo-google-fonts/poppins/poppins/Poppins_100Thin.tff",
            "node_modules/@expo-google-fonts/poppins/poppins/Poppins_200ExtraLight.tff",
            "node_modules/@expo-google-fonts/poppins/poppins/Poppins_300Light.tff",
            "node_modules/@expo-google-fonts/poppins/poppins/Poppins_400Regular.tff",
            "node_modules/@expo-google-fonts/poppins/poppins/Poppins_500Medium.tff",
            "node_modules/@expo-google-fonts/poppins/poppins/Poppins_600SemiBold.tff",
            "node_modules/@expo-google-fonts/poppins/poppins/Poppins_700Bold.tff",
            "node_modules/@expo-google-fonts/poppins/poppins/Poppins_800ExtraBold.tff",
            "node_modules/@expo-google-fonts/poppins/poppins/Poppins_900Black.tff"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
